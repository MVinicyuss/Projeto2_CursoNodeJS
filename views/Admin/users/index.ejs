<%- include ("../../partials/navbar.ejs") %>
<%- include ("../../partials/header.ejs") %>

<div class="container">
    <hr>
    <h2>Usuarios</h2>
    <a class="btn btn-success" href="/admin/users/create">Criar nova Categoria</a>
    <hr>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.id %></td>
                    <td><%= user.email %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</div>

<%- include ("../../partials/footer.ejs") %>